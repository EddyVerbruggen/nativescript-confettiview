<Page
    xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
    <StackLayout>
        <Label text="It's raining confetti!" class="message" textWrap="true"/>
        <Button text="Stop Confetti" tap="{{ stopConfetti }}"/>
        <Button text="Start Confetti" tap="{{ startConfetti }}"/>
    </StackLayout>
</Page>